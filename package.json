{
  "name": "andre",
  "version": "0.1.0",
  "description": "Andre is a clean and fast blog theme for your Hugo site.",
  "main": "index.js",
  "repository": "https://github.com/andrebarrett",
  "author": "Andre Barrett",
  "license": "MIT",
  "scripts": {
    "start": "hugo server",
    "start-examplesite": "hugo server -s exampleSite --themesDir=../.. --disableFastRender",
    "build-examplesite": "NODE_ENV=production hugo -s exampleSite --themesDir=../.. --gc",
    "build": "NODE_ENV=production hugo --gc --minify --enableGitInfo"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.4.0",
    "autoprefixer": "^10.2.5",
    "postcss": "^8.2.10",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.1",
    "tailwindcss": "^2.2.19"
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ]
}
